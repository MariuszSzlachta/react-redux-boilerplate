{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "author": "Adam Pałaszyński",
  "description": "React-Router-Redux boilerplate",
  "scripts": {
    "dev-server": "webpack-dev-server --mode development",
    "build:dev": "rm -rf ./public && webpack --mode development",
    "build:prod": "rm -rf ./public && webpack -p --env production --mode production",
    "heroku-postbuild": "npm run build:prod",
    "start": "node server/server.js"
  },
  "dependencies": {
    "autoprefixer": "^8.1.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "browser-sync": "^2.23.6",
    "browser-sync-webpack-plugin": "^2.0.1",
    "css-loader": "^0.28.10",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "github:webpack-contrib/html-webpack-plugin",
    "img-loader": "^2.0.1",
    "node-sass": "^4.8.1",
    "postcss-flexbugs-fixes": "^3.3.0",
    "postcss-loader": "^2.1.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "sass-loader": "^6.0.7",
    "svg-sprite-loader": "^3.7.1",
    "uuid": "^3.2.1",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.11",
    "webpack-dev-server": "^3.1.1"
  }
}
